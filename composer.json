{
    "name": "takatost/api-requests",
    "description": "简易 API 请求库",
    "keywords": [
      "api-requests"
    ],
    "require": {
      "php": ">=5.6.4",
      "guzzlehttp/guzzle": "~5.0|~6.0",
      "illuminate/support": ">=5.2",
      "illuminate/pagination": ">=5.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "JohnWang",
            "email": "wangjiajun@vchangyi.com"
        }
    ],
    "autoload": {
      "classmap":["src/"]
    },
    "prefer-stable": true,
    "config": {
      "preferred-install": "dist",
      "secure-http": false
    },
    "repositories": [
      {
        "type": "composer",
        "url": "https://packagist.phpcomposer.com"
      },
      {
        "packagist": false
      }
    ]
}
